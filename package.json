{
  "name": "vcl",
  "displayName": "VCL - Varnish Configuration Language",
  "description": "Syntax highlighting for Varnish Configuration Language and Ansible templates for Varnish ",
  "version": "0.1.1",
  "publisher": "thomas-baumgaertner",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vcl",
        "aliases": [
          "VCL",
          "vcl"
        ],
        "extensions": [
          ".vcl",
          ".vcl.j2"
        ],
        "configuration": "./vcl.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vcl",
        "scopeName": "source.varnish.vcl",
        "path": "./syntaxes/vcl.json"
      }
    ]
  }
}
